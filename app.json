{
    "name": "Pogo Dex",
    "description": "Web app for tracking Pokémon Go's pokédex",
    "repository": "https://github.com/ConcernedHobbit/pogo-dex",
    "buildpacks": [
        {
            "url": "heroku/python"
        }
    ],
    "addons": [
        {
            "plan": "heroku-postgresql"
        }
    ],
    "env": {
        "HEROKU": {
            "description": "Used to differentiate between local development and deployed app",
            "value": "1",
            "required": "true"
        }
    }
}